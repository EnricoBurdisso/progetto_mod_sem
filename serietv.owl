                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty serietv:numero_stagioni ;
                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                    owl:onDatatype xsd:nonNegativeInteger ;
                                                                                    owl:withRestrictions ( [ xsd:maxExclusive "2"^^xsd:nonNegativeInteger
                                                                                                           ]
                                                                                                         )
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf serietv:SerieTelevisiva ,
                                  [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                           owl:onProperty part:hasPart ;
                                                           owl:someValuesFrom serietv:Stagione
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty serietv:numero_episodi ;
                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                owl:onDatatype xsd:nonNegativeInteger ;
                                                                                owl:withRestrictions ( [ xsd:maxInclusive "11"^^xsd:nonNegativeInteger
                                                                                                       ]
                                                                                                     )
                                                                              ]
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty serietv:numero_stagioni ;
                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                owl:onDatatype xsd:nonNegativeInteger ;
                                                                                owl:withRestrictions ( [ xsd:maxExclusive "2"^^xsd:nonNegativeInteger
                                                                                                       ]
                                                                                                     )
                                                                              ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( serietv:Episodio
                                                  serietv:SerieTelevisiva
                                                )
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty part:hasPart ;
                                    owl:someValuesFrom serietv:Stagione
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty serietv:numero_episodi ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:nonNegativeInteger ;
                                                         owl:withRestrictions ( [ xsd:maxInclusive "11"^^xsd:nonNegativeInteger
                                                                                ]
                                                                              )
                                                       ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty serietv:numero_stagioni ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:nonNegativeInteger ;
                                                         owl:withRestrictions ( [ xsd:maxExclusive "2"^^xsd:nonNegativeInteger
                                                                                ]
                                                                              )
                                                       ]
                                  ] ;
                  rdfs:comment "A miniseries is a tv series where the number of episodes are at maximum 20 and they have only a single season"@en ,
                               "Una Miniserie è una serie tv dove il numero di episodi è al massimo 20 e ha una sola stagione"@it ;
                  rdfs:label "Miniserie"@it ,
