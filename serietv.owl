@prefix : <http://www.semanticweb.org/progettomodsem/2020/serietv#> .
@prefix db: <http://dbpedia.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix part: <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#> .
@prefix serietv: <http://www.semanticweb.org/progettomodsem/2020/serietv#> .
@base <http://www.semanticweb.org/progettomodsem/2020/serietv> .

<http://www.semanticweb.org/progettomodsem/2020/serietv> rdf:type owl:Ontology ;
                                                          owl:imports <http://www.ontologydesignpatterns.org/cp/owl/partof.owl> ,
                                                                      <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl> ;
                                                          <http://purl.org/dc/elements/1.1/creator> "Enrico Burdisso" ;
                                                          <http://purl.org/dc/elements/1.1/date> "2021-01-11" ;
                                                          <http://purl.org/dc/elements/1.1/description> "Ontologia che rappresenta le serie televisive"@it ,
                                                                                                        "Ontology representing television series"@en ;
                                                          <http://purl.org/dc/elements/1.1/title> "Serie televisive"@it ,
                                                                                                  "Television series"@en ;
                                                          serietv:vann:preferredNamespacePrefix "http://www.semanticweb.org/progettomodsem/2020/serietv" ;
                                                          serietv:vann:preferredNamespaceURI "serietv" ;
                                                          rdfs:label "serietv"@it ,
                                                                     "tvseries"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#vann:preferredNamespacePrefix
serietv:vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#vann:preferredNamespaceURI
serietv:vann:preferredNamespaceURI rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/progettomodsem/2020/serietv#castContienePersona
serietv:castContienePersona rdf:type owl:ObjectProperty ;
                            rdfs:domain serietv:Cast ;
                            rdfs:range serietv:Persona ,
                                       <http://xmlns.com/foaf/0.1/Person> ;
                            rdfs:comment "Property that link a cast to its person"@en ,
                                         "Proprietà che collega un cast a una persona"@it ;
                            rdfs:label "castContainsPerson"@en ,
                                       "castContienePersona"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#episodioFattoIn
serietv:episodioFattoIn rdf:type owl:ObjectProperty ;
                        rdfs:domain serietv:Episodio ;
                        rdfs:range serietv:Paese ;
                        rdfs:comment "Property that links an episode to its country"@en ,
                                     "proprietà che collega un episodio al suo paese"@it ;
                        rdfs:label "episodeDoingIn"@en ,
                                   "episodioFattoIn"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#interpreta
serietv:interpreta rdf:type owl:ObjectProperty ;
                   owl:inverseOf serietv:personaggio_interpretato_da ;
                   rdfs:domain serietv:Persona ,
                               <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:range serietv:Personaggio ;
                   rdfs:comment "Property that links an actor to its character"@en ,
                                "Proprietà che collega un attore / attrice al suo personaggio"@it ;
                   rdfs:label "PersonInterprets"@en ,
                              "PersonaInterpreta"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#persona_ha_ruolo
serietv:persona_ha_ruolo rdf:type owl:ObjectProperty ;
                         rdfs:domain serietv:Persona ,
                                     <http://xmlns.com/foaf/0.1/Person> ;
                         rdfs:range serietv:Ruolo ;
                         rdfs:comment "Property that links a person to its role in a series"@en ,
                                      "Proprietà che collega una persona al suo ruolo in una serie tv"@it ;
                         rdfs:label "personHasRole"@en ,
                                    "personaHaRuolo"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#personaggio_di_serie
serietv:personaggio_di_serie rdf:type owl:ObjectProperty ;
                             owl:inverseOf serietv:serie_ha_personaggio ;
                             rdfs:domain serietv:Personaggio ;
                             rdfs:range serietv:SerieTelevisiva ,
                                        [ rdf:type owl:Class ;
                                          owl:unionOf ( serietv:Episodio
                                                        serietv:SerieTelevisiva
                                                      )
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty part:hasPart ;
                                          owl:someValuesFrom serietv:Stagione
                                        ] ;
                             rdfs:comment "Property that links a character to its tv series"@en ,
                                          "Proprietà che collega un personaggio alla sua serie tv"@it ;
                             rdfs:label "characterOfTheSeries"@en ,
                                        "personaggioDellaSerie"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#personaggio_interpretato_da
serietv:personaggio_interpretato_da rdf:type owl:ObjectProperty ;
                                    rdfs:domain serietv:Personaggio ;
                                    rdfs:range serietv:Persona ,
                                               <http://xmlns.com/foaf/0.1/Person> ;
                                    rdfs:comment "Property that links a character to its actor"@en ,
                                                 "Proprietà che collega un personaggio al suo attore / attrice"@it ;
                                    rdfs:label "characterInterpretedBy"@en ,
                                               "personaggioInterpretatoDa"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#serieContienePersona
serietv:serieContienePersona rdf:type owl:ObjectProperty ;
                             owl:propertyChainAxiom ( serietv:serie_ha_cast
                                                      serietv:castContienePersona
                                                    ) ;
                             rdfs:comment "Property that links a series to a person"@en ,
                                          "Proprietà che collega una serie a una persona"@it ;
                             rdfs:label "seriContienePersona"@it ,
                                        "seriesContainsPerson"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#serieHaGenere
serietv:serieHaGenere rdf:type owl:ObjectProperty ;
                      rdfs:domain serietv:SerieTelevisiva ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( serietv:Episodio
                                                  serietv:SerieTelevisiva
                                                )
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty part:hasPart ;
                                    owl:someValuesFrom serietv:Stagione
                                  ] ;
                      rdfs:range db:genre ,
                                 serietv:Genere ;
                      rdfs:comment "Property that links a tv series to its genre"@en ,
                                   "Proprietà che collega una serie tv al suo genere"@it ;
                      rdfs:label "serieHaGenere"@it ,
                                 "seriesHasGenre"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#serie_ha_cast
serietv:serie_ha_cast rdf:type owl:ObjectProperty ;
                      rdfs:domain serietv:SerieTelevisiva ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( serietv:Episodio
                                                  serietv:SerieTelevisiva
                                                )
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty part:hasPart ;
                                    owl:someValuesFrom serietv:Stagione
                                  ] ;
                      rdfs:range serietv:Cast ;
                      rdfs:comment "Property that links a tv series to its cast"@en ,
                                   "Proprietà ch collega una serie tv al suo cast"@it ;
                      rdfs:label "serieHaCast"@it ,
                                 "seriesHasCast"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#serie_ha_intervallo
serietv:serie_ha_intervallo rdf:type owl:ObjectProperty ;
                            rdfs:domain serietv:SerieTelevisiva ,
                                        [ rdf:type owl:Class ;
                                          owl:unionOf ( serietv:Episodio
                                                        serietv:SerieTelevisiva
                                                      )
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty part:hasPart ;
                                          owl:someValuesFrom serietv:Stagione
                                        ] ;
                            rdfs:range time:TimeInterval ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty time:hasIntervalEndDate ;
                                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty time:hasIntervalStartDate ;
                                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                       ] ;
                            rdfs:comment "Property that liks a tv series to its time interval"@en ,
                                         "Proprietà che collega una serie tv al suo intervallo di tempo"@it ;
                            rdfs:label "serieHaIntervallo"@it ,
                                       "seriesHasInterval"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#serie_ha_paese
serietv:serie_ha_paese rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain serietv:SerieTelevisiva ,
                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( serietv:Episodio
                                                   serietv:SerieTelevisiva
                                                 )
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty part:hasPart ;
                                     owl:someValuesFrom serietv:Stagione
                                   ] ;
                       rdfs:range serietv:Paese ,
                                  [ rdf:type owl:Class ;
                                    owl:oneOf ( serietv:Brasile
                                                serietv:Italia
                                                serietv:USA
                                              )
                                  ] ;
                       rdfs:comment "Functional property that links a tv series to its original country"@en ,
                                    "Proprietà funzionale che collega una serie tv al suo paese d' origine"@it ;
                       rdfs:label "serieHaPaese"@it ,
                                  "seriesHasCountry"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#serie_ha_personaggio
serietv:serie_ha_personaggio rdf:type owl:ObjectProperty ;
                             rdfs:domain serietv:SerieTelevisiva ,
                                         [ rdf:type owl:Class ;
                                           owl:unionOf ( serietv:Episodio
                                                         serietv:SerieTelevisiva
                                                       )
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty part:hasPart ;
                                           owl:someValuesFrom serietv:Stagione
                                         ] ;
                             rdfs:range serietv:Personaggio ;
                             rdfs:comment "Property that links a tv series to its character"@en ,
                                          "Proprietà che collega una serie tv al suo personaggio"@it ;
                             rdfs:label "serieHaPersonaggio"@it ,
                                        "seriesHasCharacter"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalDate
time:hasIntervalDate rdfs:domain [ rdf:type owl:Restriction ;
                                   owl:onProperty time:hasIntervalEndDate ;
                                   owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty time:hasIntervalStartDate ;
                                   owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                 ] .


###  http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalEndDate
time:hasIntervalEndDate rdfs:domain [ rdf:type owl:Restriction ;
                                      owl:onProperty time:hasIntervalEndDate ;
                                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty time:hasIntervalStartDate ;
                                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                    ] .


###  http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalStartDate
time:hasIntervalStartDate rdfs:domain [ rdf:type owl:Restriction ;
                                        owl:onProperty time:hasIntervalEndDate ;
                                        owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty time:hasIntervalStartDate ;
                                        owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                      ] .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#conclusa
serietv:conclusa rdf:type owl:DatatypeProperty ;
                 rdfs:domain serietv:SerieTelevisiva ,
                             [ rdf:type owl:Class ;
                               owl:unionOf ( serietv:Episodio
                                             serietv:SerieTelevisiva
                                           )
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty part:hasPart ;
                               owl:someValuesFrom serietv:Stagione
                             ] ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Property that says if a series is ended or not"@en ,
                              "Proprietà che dice se una serie è conclusa o no"@it ;
                 rdfs:label "conclusa"@it ,
                            "ended"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#numero_episodi
serietv:numero_episodi rdf:type owl:DatatypeProperty ;
                       rdfs:domain serietv:SerieTelevisiva ,
                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( serietv:Episodio
                                                   serietv:SerieTelevisiva
                                                 )
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty part:hasPart ;
                                     owl:someValuesFrom serietv:Stagione
                                   ] ;
                       rdfs:range xsd:nonNegativeInteger ;
                       rdfs:comment "number of episodes of a tv series"@en ,
                                    "numero di episodi di una serie tv"@it ;
                       rdfs:label "numberEpisodes"@en ,
                                  "numeroEpisodi"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#numero_stagioni
serietv:numero_stagioni rdf:type owl:DatatypeProperty ;
                        rdfs:domain serietv:SerieTelevisiva ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( serietv:Episodio
                                                    serietv:SerieTelevisiva
                                                  )
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty part:hasPart ;
                                      owl:someValuesFrom serietv:Stagione
                                    ] ;
                        rdfs:range xsd:nonNegativeInteger ;
                        rdfs:comment "number of seasons of a tv series"@en ,
                                     "numero di stagioni di una serie tv"@it ;
                        rdfs:label "numberSeasons"@en ,
                                   "numeroStagioni"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#titolo
serietv:titolo rdf:type owl:DatatypeProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( serietv:Episodio
                                           serietv:SerieTelevisiva
                                         )
                           ] ;
               rdfs:comment "title of an episode or a tv series"@en ,
                            "titolo di un episodio o di una serie tv"@it ;
               rdfs:label "title"@en ,
                          "titolo"@it .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology#genre
db:genre rdf:type owl:Class ;
         owl:equivalentClass serietv:Genere ;
         rdfs:subClassOf serietv:Genere ;
         rdfs:comment "Genere definito da dbpedia"@it ,
                      "Genre defined by dbpedia"@en ;
         rdfs:label "dbo:genre"@en ,
                    "dbo:genre"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Attore
serietv:Attore rdf:type owl:Class ;
               rdfs:subClassOf serietv:Ruolo ;
               rdfs:comment "Role as an actor / actress of a tv series"@en ,
                            "Ruolo da attore / attrice in una serie tv"@it ;
               rdfs:label "Actor"@en ,
                          "Attore"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#AttoreSerie
serietv:AttoreSerie rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty serietv:persona_ha_ruolo ;
                                          owl:someValuesFrom serietv:Attore
                                        ] ;
                    rdfs:subClassOf serietv:Persona ,
                                    <http://xmlns.com/foaf/0.1/Person> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty serietv:persona_ha_ruolo ;
                                      owl:someValuesFrom serietv:Attore
                                    ] ;
                    rdfs:comment "Person who has role of actor / actress of a tv series"@en ,
                                 "Persona che ha ruolo di attore / attrice in una serie tv"@it ;
                    rdfs:label "AttoreSerie"@it ,
                               "SeriesActor"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Brasile
serietv:Brasile rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( serietv:Brasile
                                                )
                                    ] ;
                rdfs:subClassOf serietv:Paese ,
                                [ rdf:type owl:Class ;
                                  owl:oneOf ( serietv:Brasile
                                            )
                                ] ,
                                [ rdf:type owl:Class ;
                                  owl:oneOf ( serietv:Brasile
                                              serietv:Italia
                                              serietv:USA
                                            )
                                ] .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Cast
serietv:Cast rdf:type owl:Class ;
             rdfs:comment "I vari cast delle serie tv"@it ,
                          "Various cast of the tv series"@en ;
             rdfs:label "Cast"@en ,
                        "Cast"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#CastMin3Persone
serietv:CastMin3Persone rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty serietv:castContienePersona ;
                                              owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                              owl:onClass serietv:Persona
                                            ] ;
                        rdfs:subClassOf serietv:Cast ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty serietv:castContienePersona ;
                                          owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                          owl:onClass serietv:Persona
                                        ] ;
                        rdfs:comment "Class that contains cast instances that have a minimum of 3 people"@en ,
                                     "Classe che contiene istanze di cast che hanno minimo 3 persone"@it ;
                        rdfs:label "CastMin3People"@en ,
                                   "CastMin3Persone"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Commedia
serietv:Commedia rdf:type owl:Class ;
                 rdfs:subClassOf db:genre ,
                                 serietv:Genere ;
                 rdfs:comment "Commedy genre"@en ,
                              "Genere commedia"@it ;
                 rdfs:label "Commedia"@it ,
                            "Commedy"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Dramma
serietv:Dramma rdf:type owl:Class ;
               rdfs:subClassOf db:genre ,
                               serietv:Genere ;
               rdfs:comment "Drama genre"@en ,
                            "Genere dramma"@it ;
               rdfs:label "Drama"@en ,
                          "Dramma"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#DrammaFamigliare
serietv:DrammaFamigliare rdf:type owl:Class ;
                         rdfs:subClassOf db:genre ,
                                         serietv:Dramma ,
                                         serietv:Genere ;
                         rdfs:comment "Familiar drama genre"@en ,
                                      "Genere dramma famigliare"@it ;
                         rdfs:label "DrammaFamigliare"@it ,
                                    "FamiliarDrama"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Drammatico
serietv:Drammatico rdf:type owl:Class ;
                   rdfs:subClassOf db:genre ,
                                   serietv:Genere ;
                   rdfs:comment "Dramatic genre"@en ,
                                "genere drammatico"@it ;
                   rdfs:label "Drammatic"@en ,
                              "Drammatico"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Episodio
serietv:Episodio rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Class ;
                                   owl:unionOf ( serietv:Episodio
                                                 serietv:SerieTelevisiva
                                               )
                                 ] ;
                 rdfs:comment "Episode of a season that belong to a tv series"@en ,
                              "Episodio di una stagione appartenente a una serie tv"@it ;
                 rdfs:label "Episode"@en ,
                            "Episodio"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Fantascienza
serietv:Fantascienza rdf:type owl:Class ;
                     rdfs:subClassOf db:genre ,
                                     serietv:Genere ;
                     rdfs:comment "Science fiction genre"@en ,
                                  "genere fantascienza"@it ;
                     rdfs:label "Fantascienza"@it ,
                                "ScienceFiction"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Genere
serietv:Genere rdf:type owl:Class ;
               rdfs:subClassOf db:genre ;
               rdfs:comment "Genere di una serie tv"@it ,
                            "Genre of a tv series"@en ;
               rdfs:label "Genere"@it ,
                          "Genre"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Ideatore
serietv:Ideatore rdf:type owl:Class ;
                 rdfs:subClassOf serietv:Ruolo ;
                 rdfs:comment "Role as a creator of a tv series"@en ,
                              "Ruolo di un ideatore / ideatrice nella serie tv"@it ;
                 rdfs:label "CreatedBy"@en ,
                            "Ideatore"@it .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#IdeatoreSerie
serietv:IdeatoreSerie rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty serietv:persona_ha_ruolo ;
                                            owl:someValuesFrom serietv:Ideatore
                                          ] ;
                      rdfs:subClassOf serietv:Persona ,
                                      <http://xmlns.com/foaf/0.1/Person> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty serietv:persona_ha_ruolo ;
                                        owl:someValuesFrom serietv:Ideatore
                                      ] ;
                      rdfs:comment "Person who has role of creator of a tv series"@en ,
                                   "Persona che ha ruolo di ideatore / ideatrice di una serie tv"@it ;
                      rdfs:label "IdeatoreSerie"@it ,
                                 "SeriesCreatedBy"@en .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Italia
serietv:Italia rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( serietv:Italia
                                               )
                                   ] ;
               rdfs:subClassOf serietv:Paese ,
                               [ rdf:type owl:Class ;
                                 owl:oneOf ( serietv:Brasile
                                             serietv:Italia
                                             serietv:USA
                                           )
                               ] ,
                               [ rdf:type owl:Class ;
                                 owl:oneOf ( serietv:Italia
                                           )
                               ] .


###  http://www.semanticweb.org/progettomodsem/2020/serietv#Medico
serietv:Medico rdf:type owl:Class ;
